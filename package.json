{
  "name": "st8",
  "version": "0.0.4",
  "description": "Control state of any object",
  "main": "index.js",
  "dependencies": {
    "polymer-weakmap": "latest",
    "enot": "latest",
    "mutypes": "latest",
    "each-csv": "latest"
  },
  "devDependencies": {
    "chai": "~1.9.1",
    "mocha": "~1.21.4",
    "require-stub": "latest"
  },
  "scripts": {
    "build": "npm run min-index && browserify index.min.js --standalone enot > dist/enot.js && npm run minify && npm test",
    "test": "mocha test/index.js",
    "minify": "ccjs dist/enot.js --compilation_level=SIMPLE --language_in=ECMASCRIPT5_STRICT > dist/enot.min.js",
    "min-index": "ccjs index.js --language_in=ECMASCRIPT5_STRICT --compilation_level=ADVANCED --formatting=PRETTY_PRINT > index.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dfcreative/st8"
  },
  "keywords": [
    "state",
    "fsm"
  ],
  "author": "Deema Evanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfcreative/st8/issues"
  }
}
